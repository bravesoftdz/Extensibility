<?xml version="1.0"?>
<root>
  <OBJECT NAME="ACER">
    <METHOD NAME="ListaTickets" DESCRIPTION="Lista Tickets" VERSION="34" INTFTYPE="5" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP NAME="Per&#237;odo"/>
          <GROUP NAME="Detalhes"/>
        </GROUPS>
        <PARAM NAME="DATA_INICIAL" FORMAT="D" SIZE="10" FLAGS="1" DEFAULT="today" PROJECTION="0" ORDER="1" FIELDLABEL="Data Inicial" GROUPNAME="Per&#237;odo" CTAB="0"/>
        <PARAM NAME="DATA_FINAL" FORMAT="D" SIZE="10" FLAGS="1" DEFAULT="today" PROJECTION="0" ORDER="2" FIELDLABEL="Data Final" GROUPNAME="Per&#237;odo" CTAB="0"/>
        <PARAM NAME="EVENTTYPE" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="4" LOOKUP="list:'INBOUND','InBound';'ISSUE','Issue';'UNISSUE','UnIssue';'OUTBOUND','OutBound';" FIELDLABEL="Tipo" GROUPNAME="Detalhes" CTAB="0"/>
        <PARAM NAME="CSSTICKETNUMBER" SIZE="30" FLAGS="1" PROJECTION="0" ORDER="5" FIELDLABEL="N&#176; Ticket" GROUPNAME="Detalhes" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="EVENTHEADER" FORMAT="N" SIZE="8" PROJECTION="0" ORDER="9" FIELDLABEL="Eventheader" CTAB="0"/>
        <FIELD NAME="EVENTTYPE" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="7" FIELDLABEL="Tipo" CTAB="0"/>
        <FIELD NAME="DATA" FORMAT="H" SIZE="18" FLAGS="1" PROJECTION="0" ORDER="8" FIELDLABEL="Data" CTAB="0"/>
        <FIELD NAME="CUSTOMERNAME" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="10" FIELDLABEL="Cliente" CTAB="0"/>
        <FIELD NAME="CSSTICKETNUMBER" SIZE="50" FLAGS="1" PROJECTION="0" ORDER="9" FIELDLABEL="N&#176; Ticket" CTAB="0"/>
        <FIELD NAME="PROCESSED" FORMAT="B" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="11" FIELDLABEL="Processado" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT EVENTHEADER,
       EVENTTYPE,
       DATA,
       CUSTOMERNAME,
       CSSTICKETNUMBER,
       #IF((COALESCE(ORDEM_SERVICO,0)=0),FALSE,TRUE) AS PROCESSED
FROM ACER_TICKETEVENTHEADER
WHERE [EXTRACTDATE(DATA) BETWEEN :DATA_INICIAL AND :DATA_FINAL]
      [AND UPPER(EVENTTYPE)=:EVENTTYPE]
      [AND CSSTICKETNUMBER=:CSSTICKETNUMBER]
ORDER BY EVENTHEADER
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>