<?xml version="1.0"?>
<root>
  <OBJECT NAME="MOEDAS">
    <METHOD NAME="Lista" DESCRIPTION="Listar Moedas" VERSION="2" INTFTYPE="5" THREADSAFE="1">
      <PARAMS/>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="MOEDA" FORMAT="N" SIZE="8" FLAGS="8" PROJECTION="0" FIELDLABEL="Moeda" CTAB="0"/>
        <FIELD NAME="DESCRICAO" SIZE="30" FLAGS="17" PROJECTION="0" ORDER="1" FIELDLABEL="Descricao" CTAB="0"/>
        <FIELD NAME="PERIODO_ATUALIZACAO" FORMAT="N" SIZE="8" FLAGS="9" PROJECTION="0" ORDER="2" FIELDLABEL="Periodo Atualizacao" CTAB="0"/>
        <FIELD NAME="MOEDA_PADRAO" FORMAT="B" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Moeda Padrao" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT MOEDA, DESCRICAO, PERIODO_ATUALIZACAO, MOEDA_PADRAO 
FROM MOEDAS 
ORDER BY DESCRICAO
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>