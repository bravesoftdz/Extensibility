<?xml version="1.0"?>
<root>
  <OBJECT NAME="MARCAS">
    <METHOD NAME="Listar" DESCRIPTION="" VERSION="2" TRIGGEROF="PRODUTOSAC.Lista" TRIGGERTYPE="1" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="COD_MARCA" SIZE="15" FLAGS="1" PROJECTION="0" FIELDLABEL="Cod Marca" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="MARCA" FORMAT="+" SIZE="8" FLAGS="1048" PROJECTION="0" FIELDLABEL="Marca" CTAB="0"/>
        <FIELD NAME="DESCRICAO" SIZE="45" FLAGS="1" PROJECTION="0" FIELDLABEL="Descricao" CTAB="0"/>
        <FIELD NAME="COD_MARCA" SIZE="15" FLAGS="1" PROJECTION="0" FIELDLABEL="Cod Marca" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT MARCA, COD_MARCA, DESCRICAO
 FROM MARCAS
 WHERE [ COD_MARCA = :COD_MARCA ]
 ORDER BY DESCRICAO;

</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
