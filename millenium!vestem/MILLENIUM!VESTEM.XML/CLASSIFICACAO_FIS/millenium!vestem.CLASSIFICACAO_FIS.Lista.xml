          <GROUP/>
        </GROUPS>
        <FIELD NAME="CLASSIFICACAO_FIS" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Classificacao Fis" CTAB="0"/>
        <FIELD NAME="CODIGO" SIZE="3" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="C&#243;digo" CTAB="0"/>
