<?xml version="1.0"?>
<root>
  <OBJECT NAME="CLASSIFICACAO_FIS">
    <METHOD NAME="Lista" DESCRIPTION="Listar Classifica&#231;&#245;es Fiscais" VERSION="21" INTFTYPE="5" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP/>
