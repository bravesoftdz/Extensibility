<?xml version="1.0"?>
<root>
  <OBJECT NAME="PLANO_CONTAS">
    <METHOD NAME="Lista_Filhos" DESCRIPTION="" VERSION="0" THREADSAFE="1">
      <PARAMS/>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="PCONTA" FORMAT="N" SIZE="8" FLAGS="8" PROJECTION="0" FIELDLABEL="Pconta" CTAB="0"/>
        <FIELD NAME="DESCRICAO" SIZE="60" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Descricao" CTAB="0"/>
        <FIELD NAME="CLASSIFICACAO" SIZE="15" FLAGS="1" PROJECTION="0" ORDER="2" FIELDLABEL="Classificacao" CTAB="0"/>
        <FIELD NAME="CODIGO" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Codigo" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT  PCONTA, DESCRICAO, CLASSIFICACAO, CODIGO, VALOR_PREVISTO
FROM PLANO_CONTAS 
WHERE EHPAI = "F"
AND PCONTA &lt;> -2000000000
ORDER BY CLASSIFICACAO
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>