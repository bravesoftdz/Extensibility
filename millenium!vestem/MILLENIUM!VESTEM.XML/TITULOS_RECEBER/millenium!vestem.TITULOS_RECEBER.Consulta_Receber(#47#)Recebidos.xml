<?xml version="1.0"?>
<root>
  <OBJECT NAME="TITULOS_RECEBER">
    <METHOD NAME="Consulta_Receber/Recebidos" DESCRIPTION="" VERSION="15" INTFTYPE="5" TRIGGEROF="TITULOS_RECEBER.Consulta_Receber/Recebidos" TRIGGERTYPE="0" CATEGORY="FINANCEIRO" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="PREVISAO" FORMAT="B" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Previsao" CTAB="0"/>
        <PARAM NAME="PRORROGADO" FORMAT="B" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="5" FIELDLABEL="Prorrogado" CTAB="0"/>
        <PARAM NAME="DEVOLVIDO" FORMAT="B" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Devolvido" CTAB="0"/>
        <PARAM NAME="CARTORIO" FORMAT="B" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Cartorio" CTAB="0"/>
        <PARAM NAME="PROTESTO" FORMAT="B" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="6" FIELDLABEL="Protesto" CTAB="0"/>
        <PARAM NAME="CONCILIADO" FORMAT="B" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="2" FIELDLABEL="Conciliado" CTAB="0"/>
      </PARAMS>
      <FIELDS/>
      <ACTIONSCRIPT>SELECT TMP.*, L.PREVISAO
FROM #REPLACE(:PARENT_RESULT) TMP

INNER JOIN LANCAMENTOS L ON L.LANCAMENTO=TMP.LANCAMENTO

WHERE [L.PREVISAO=:PREVISAO]
      [AND L.PRORROGADO=:PRORROGADO]
      [AND L.DEVOLVIDO=:DEVOLVIDO]
      [AND L.CARTORIO=:CARTORIO]
      [AND L.PROTESTO=:PROTESTO]
      [AND L.CONCILIADO=:CONCILIADO];

</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>