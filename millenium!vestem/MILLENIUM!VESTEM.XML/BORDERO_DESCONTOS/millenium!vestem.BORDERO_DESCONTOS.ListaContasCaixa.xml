<?xml version="1.0"?>
<root>
  <OBJECT NAME="BORDERO_DESCONTOS">
    <METHOD NAME="ListaContasCaixa" DESCRIPTION="" VERSION="0" THREADSAFE="1">
      <PARAMS/>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="CONTA" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Conta" CTAB="0"/>
        <FIELD NAME="DESCRICAO" SIZE="40" FLAGS="1" PROJECTION="0" ORDER="2" FIELDLABEL="Descricao" CTAB="0"/>
        <FIELD NAME="NUMERO" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Numero" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT CONTA, BANCOS.BANCO,
       NUMERO,
       DESCRICAO,
       CONTA AS SCRIPTCONTA,
       FILIAL

FROM CONTAS

INNER JOIN BANCOS
  ON CONTAS.BANCO = BANCOS.BANCO

WHERE CONTAS.TIPOC &lt;> 'X'
  AND BANCOS.COD_BANCO = '104'


ORDER BY DESCRICAO
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>