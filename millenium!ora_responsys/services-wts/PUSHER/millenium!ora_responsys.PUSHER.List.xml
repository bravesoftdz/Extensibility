<?xml version="1.0"?>
<root>
  <OBJECT NAME="PUSHER">
    <METHOD NAME="List" DESCRIPTION="Lista de Publica&#231;&#245;es Responsys" VERSION="23" INTFTYPE="14" THREADSAFE="1">
      <PARAMS/>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="ORARES_PUSHER" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Orares Pusher" CTAB="0"/>
        <FIELD NAME="DESCRIPTION" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Descri&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="LIST_INFORMATION" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="2" FIELDLABEL="Lista Informa&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="TRANS_ID" FORMAT="N" SIZE="8" PROJECTION="0" ORDER="3" FIELDLABEL="Trans Id" CTAB="0"/>
        <FIELD NAME="DATE_PUBLISHER" FORMAT="H" SIZE="18" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="&#218;ltima Publica&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="COUNT_ERROR" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Qtd. Erro" CTAB="0"/>
        <FIELD NAME="COUNT_PUBLISHED" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="5" FIELDLABEL="Qtd. Publicado" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT P.ORARES_PUSHER,
       P.DESCRIPTION,
       P.LIST_INFORMATION,
       P.TRANS_ID,
       P.DATE_PUBLISHER,
       (SELECT COUNT(*) FROM ORARES_PUSHER_LOGS PL WHERE (PL.ORARES_PUSHER = P.ORARES_PUSHER) AND (PL.ID_RESPONSYS = '-1')) AS  COUNT_ERROR,
       (SELECT COUNT(*) FROM ORARES_PUSHER_LOGS PL WHERE (PL.ORARES_PUSHER = P.ORARES_PUSHER) AND (PL.ID_RESPONSYS &lt;> '-1')) AS  COUNT_PUBLISHED
FROM ORARES_PUSHER P
ORDER BY ORARES_PUSHER;
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>