<?xml version="1.0"?>
<root>
  <OBJECT NAME="PLANEJAMENTOS_ESTOQUES">
    <METHOD NAME="ListarProdutos" DESCRIPTION="" VERSION="3" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="PLANEJAMENTO_ESTOQUE" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Planejamento Estoque" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="COD_PRODUTO" SIZE="20" PROJECTION="0" FIELDLABEL="Cod Produto" CTAB="0"/>
        <FIELD NAME="PLAN_ESTOQUE_PRODUTO" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Plan Estoque Produto" CTAB="0"/>
        <FIELD NAME="PRODUTO" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Produto" CTAB="0"/>
        <FIELD NAME="COR" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Cor" CTAB="0"/>
        <FIELD NAME="ESTAMPA" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Estampa" CTAB="0"/>
        <FIELD NAME="TAMANHO" SIZE="5" PROJECTION="0" FIELDLABEL="Tamanho" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT P.COD_PRODUTO,
       QP.PLAN_ESTOQUE_PRODUTO,
       QP.PRODUTO,
       QP.COR,
       QP.ESTAMPA,
       QP.TAMANHO
FROM QIX_PLAN_ESTOQUE_PRODUTOS QP
INNER JOIN PRODUTOS P ON (QP.PRODUTO = P.PRODUTO)
WHERE QP.PLANEJAMENTO_ESTOQUE=:PLANEJAMENTO_ESTOQUE
ORDER BY P.PRODUTO
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>