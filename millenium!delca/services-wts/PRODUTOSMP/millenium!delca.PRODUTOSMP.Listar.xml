<?xml version="1.0"?>
<root>
  <OBJECT NAME="PRODUTOSMP">
    <METHOD NAME="Listar" DESCRIPTION="" VERSION="20" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="PRODUTO" FORMAT="N" SIZE="8" FLAGS="2" PROJECTION="0" FIELDLABEL="Produto" CTAB="0"/>
        <PARAM NAME="TABELA_CUSTO" FORMAT="N" SIZE="8" FLAGS="2" PROJECTION="0" FIELDLABEL="Tabela Custo" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="PRODUTO" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Produto" CTAB="0"/>
        <FIELD NAME="COD_PRODUTO" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Cod Produto" CTAB="0"/>
        <FIELD NAME="DESCRICAO1" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="2" FIELDLABEL="Descricao" CTAB="0"/>
        <FIELD NAME="UNIDADE_MEDIDA" SIZE="2" FLAGS="3" PROJECTION="0" ORDER="5" FIELDLABEL="Unidade Uso" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT P.PRODUTO,
       P.COD_PRODUTO,
       P.DESCRICAO1,
       P.UNIDADE_USO AS UNIDADE_MEDIDA
FROM PRODUTOS P
WHERE P.TIPO_PROD='MP'
GROUP BY P.PRODUTO,
         P.COD_PRODUTO,
         P.DESCRICAO1,
         P.UNIDADE_USO
ORDER BY P.DESCRICAO1
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>