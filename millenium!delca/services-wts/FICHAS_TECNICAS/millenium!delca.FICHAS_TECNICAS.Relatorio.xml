<?xml version="1.0"?>
<root>
  <OBJECT NAME="FICHAS_TECNICAS">
    <METHOD NAME="Relatorio" DESCRIPTION="" VERSION="8" TRIGGEROF="FICHAS_TECNICAS.CONSULTA" TRIGGERTYPE="0" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="PRODUTO" FORMAT="N" SIZE="8" FLAGS="1" STYLE="5" PROJECTION="0" ORDER="1" LOOKUP="MILLENIUM.PRODUTOS.LISTA_POPUP3" LOOKUPKEY="PRODUTO" LOOKUPDISPLAY="DESCRICAO" LOOKUPCODE="CODIGO" FIELDLABEL="Produto" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="SEQUENCIA" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="9" FIELDLABEL="Sequencia" CTAB="0"/>
        <FIELD NAME="FREQ_INSPECAO" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="8" FIELDLABEL="Freq Inspecao" CTAB="0"/>
        <FIELD NAME="FREQ_ANOTACAO" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="7" FIELDLABEL="Freq Anotacao" CTAB="0"/>
        <FIELD NAME="TECNICA_MEDICAO" SIZE="50" FLAGS="1" PROJECTION="0" ORDER="10" FIELDLABEL="Tecnica Medicao" CTAB="0"/>
        <FIELD NAME="DIMENSAO" SIZE="50" FLAGS="1" PROJECTION="0" ORDER="5" FIELDLABEL="Dimensao" CTAB="0"/>
        <FIELD NAME="ESPECIFICACAO" SIZE="50" FLAGS="1" PROJECTION="0" ORDER="6" FIELDLABEL="Especificacao" CTAB="0"/>
        <FIELD NAME="COD_PRODUTO" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Cod Produto" CTAB="0"/>
        <FIELD NAME="DESC_PRODUTO" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Desc Produto" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT P.COD_PRODUTO,
       P.DESCRICAO1 AS DESC_PRODUTO,
       I.SEQUENCIA,
       I.FREQ_INSPECAO,
       I.FREQ_ANOTACAO,
       I.TECNICA_MEDICAO,
       I.DIMENSAO,
       I.ESPECIFICACAO
FROM PRODUTOS P
INNER JOIN DCA_FREQUENCIAS_INSPECOES I ON (P.PRODUTO = I.PRODUTO)
WHERE [P.PRODUTO=:PRODUTO]
ORDER BY I.SEQUENCIA
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>