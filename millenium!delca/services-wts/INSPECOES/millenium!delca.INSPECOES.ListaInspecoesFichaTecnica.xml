<?xml version="1.0"?>
<root>
  <OBJECT NAME="INSPECOES">
    <METHOD NAME="ListaInspecoesFichaTecnica" DESCRIPTION="" VERSION="38" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="FICHA_TECNICA" FORMAT="N" SIZE="8" FLAGS="2" PROJECTION="0" FIELDLABEL="Ficha Tecnica" CTAB="0"/>
        <PARAM NAME="PRODUCAO" FORMAT="N" SIZE="8" FLAGS="2" PROJECTION="0" FIELDLABEL="Producao" CTAB="0"/>
        <PARAM NAME="FASE" FORMAT="N" SIZE="8" FLAGS="2" PROJECTION="0" FIELDLABEL="Fase" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="DESCRICAO" SIZE="50" FLAGS="129" PROJECTION="0" ORDER="2" FIELDLABEL="Descricao" CTAB="0"/>
        <FIELD NAME="ESPECIFICACAO" SIZE="50" FLAGS="129" PROJECTION="0" ORDER="4" FIELDLABEL="Especificacao" CTAB="0"/>
        <FIELD NAME="FERRAMENTA" SIZE="50" FLAGS="129" PROJECTION="0" ORDER="5" FIELDLABEL="Ferramenta" CTAB="0"/>
        <FIELD NAME="OBSERVACAO" SIZE="100" FLAGS="129" PROJECTION="0" ORDER="9" FIELDLABEL="Observacao" CTAB="0"/>
        <FIELD NAME="ENCONTRADO" SIZE="50" FLAGS="17" PROJECTION="0" ORDER="3" FIELDLABEL="Encontrado" CTAB="0"/>
        <FIELD NAME="MINIMA" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="7" FIELDLABEL="Minima" CTAB="0"/>
        <FIELD NAME="MAXIMA" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="6" FIELDLABEL="Maxima" CTAB="0"/>
        <FIELD NAME="APROVADO" FORMAT="B" SIZE="1" FLAGS="17" PROJECTION="0" ORDER="1" FIELDLABEL="Aprovado" CTAB="0"/>
        <FIELD NAME="OBSERVACAO_INSP" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="8" FIELDLABEL="Obs" CTAB="0"/>
        <FIELD NAME="FICHA_TEC_INSP" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" FIELDLABEL="Ficha Tec Insp" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT  FT.FICHA_TEC_INSP,
        FT.DESCRICAO,
        FT.ESPECIFICACAO,
        F.DESCRICAO AS FERRAMENTA,
        FT.OBSERVACAO,
        NULL AS ENCONTRADO,
        NULL AS MINIMA,
        NULL AS MAXIMA,
        FALSE AS APROVADO,
        NULL OBSERVACAO_INSP
FROM DCA_FICHAS_TEC_INSP FT
INNER JOIN DCA_FERRAMENTAS F ON  (F.FERRAMENTA = FT.FERRAMENTA)
WHERE FT.FICHA_TECNICA=:FICHA_TECNICA
ORDER BY FT.FICHA_TEC_INSP;
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>