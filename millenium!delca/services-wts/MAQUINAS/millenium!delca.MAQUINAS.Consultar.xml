        <FIELD NAME="DCA_VALOR_DEPRECIACAO" FORMAT="M" SIZE="17" FLAGS="1" DECIMALS="4" PROJECTION="0" ORDER="1" FIELDLABEL="Deprecia&#231;&#227;o" GROUPNAME="Valores Hora" CTAB="0"/>
        <FIELD NAME="DCA_VALOR_ENERGIA" FORMAT="M" SIZE="17" FLAGS="1" DECIMALS="4" PROJECTION="0" ORDER="2" FIELDLABEL="Energia" GROUPNAME="Valores Hora" CTAB="0"/>
        <FIELD NAME="DCA_VALOR_OUTROS" FORMAT="M" SIZE="17" FLAGS="1" DECIMALS="4" PROJECTION="0" ORDER="3" FIELDLABEL="Outros" GROUPNAME="Valores Hora" CTAB="0"/>
        <FIELD NAME="DCA_VALOR_MAQUINA" FORMAT="M" SIZE="17" FLAGS="129" DECIMALS="4" EXPRESSION="DCA_VALOR_DEPRECIACAO+DCA_VALOR_ENERGIA+DCA_VALOR_OUTROS" PROJECTION="0" ORDER="4" FIELDLABEL="M&#225;quina" GROUPNAME="Valores Hora" CTAB="0"/>
