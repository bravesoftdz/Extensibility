    <METHOD NAME="Consultar" DESCRIPTION="" VERSION="49" INTFTYPE="6" TRIGGEROF="MAQUINAS.CONSULTAR" TRIGGERTYPE="0" THREADSAFE="0">
      <PARAMS/>
      <FIELDS>
