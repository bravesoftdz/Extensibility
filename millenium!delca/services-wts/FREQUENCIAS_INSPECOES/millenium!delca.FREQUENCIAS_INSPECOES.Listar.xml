<?xml version="1.0"?>
<root>
  <OBJECT NAME="FREQUENCIAS_INSPECOES">
    <METHOD NAME="Listar" DESCRIPTION="Lista Frequencia de Inspe&#231;&#245;es" VERSION="21" INTFTYPE="5" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="PRODUTO" FORMAT="N" SIZE="8" FLAGS="1" STYLE="5" PROJECTION="0" ORDER="1" LOOKUP="MILLENIUM.PRODUTOS.LISTA_POPUP3" LOOKUPKEY="PRODUTO" LOOKUPDISPLAY="DESCRICAO" LOOKUPCODE="CODIGO" FIELDLABEL="Produto" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="MIN_PROD" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Min Prod" CTAB="0"/>
        <FIELD NAME="PRODUTO" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Ficha Tecnica" CTAB="0"/>
        <FIELD NAME="COD_PRODUTO" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Produto" CTAB="0"/>
        <FIELD NAME="DESC_PRODUTO" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Descri&#231;&#227;o Produto" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT DISTINCT
       P.PRODUTO AS MIN_PROD,
       P.PRODUTO,
       P.COD_PRODUTO,
       P.DESCRICAO1 AS DESC_PRODUTO
FROM DCA_FREQUENCIAS_INSPECOES FI
INNER JOIN PRODUTOS P ON (P.PRODUTO = FI.PRODUTO)
WHERE [P.PRODUTO=:PRODUTO]
ORDER BY P.COD_PRODUTO
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>