<?xml version="1.0"?>
<root>
  <OBJECT NAME="SOLICITACOES_COMPRA">
    <METHOD NAME="Consultar" DESCRIPTION="Consultar Incluir Solicita&#231;&#227;o de Compra" VERSION="142" INTFTYPE="6" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="SOLICITACAO_COMPRA" FORMAT="+" SIZE="8" FLAGS="1024" PROJECTION="0" FIELDLABEL="Solicitacao Compra" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
          <GROUP NAME="Controle Or&#231;ament&#225;rio"/>
          <GROUP NAME="Indica Fornecedor"/>
          <GROUP NAME="Produtos"/>
          <GROUP/>
          <GROUP NAME="Cota&#231;&#245;es" STYLE="1"/>
          <GROUP NAME="Finaliza&#231;&#227;o Compra"/>
          <GROUP NAME="Fluxo" STYLE="1"/>
        </GROUPS>
        <FIELD NAME="SOLICITACAO_COMPRA" FORMAT="+" SIZE="8" FLAGS="1024" PROJECTION="0" FIELDLABEL="Solicitacao Compra" CTAB="0"/>
        <FIELD NAME="COD_SOLICITACAO" SIZE="10" FLAGS="145" DEFAULT="millenium.utils.default(['SF_COD_SOLICITACAO_COMPRA',5],'Result',True)" PROJECTION="0" ORDER="1" FIELDLABEL="N&#250;mero" CTAB="0"/>
        <FIELD NAME="DATA" FORMAT="D" SIZE="10" FLAGS="145" DEFAULT="Today" PROJECTION="0" ORDER="2" FIELDLABEL="Data" CTAB="0"/>
        <FIELD NAME="FUNCIONARIO" FORMAT="N" SIZE="8" FLAGS="17" ENABLINGRULE="ALTERACAO=FALSE" PROJECTION="0" ORDER="4" LOOKUP="MILLENIUM.FUNCIONARIOS.LISTA_TODOS" LOOKUPKEY="FUNCIONARIO" LOOKUPDISPLAY="NOME" LOOKUPCODE="COD_FUNCIONARIO" FIELDLABEL="Funcion&#225;rio" CTAB="0"/>
        <FIELD NAME="EMERGENCIAL" FORMAT="B" SIZE="1" FLAGS="17" ENABLINGRULE="ALTERACAO=FALSE" DEFAULT="0" PROJECTION="0" ORDER="5" FIELDLABEL="Emergencial" CTAB="0"/>
        <FIELD NAME="TIPO_PRODUTO" FORMAT="N" SIZE="8" FLAGS="17" ENABLINGRULE="ALTERACAO=FALSE" DEFAULT="1" PROJECTION="0" ORDER="6" LOOKUP="list:'0','Servi&#231;o';'1','Material de Consumo';'2','Contrato';" FIELDLABEL="Tipo Produto" CTAB="0"/>
        <FIELD NAME="PRODUTOS" FORMAT="R" FLAGS="1" VISIBILITYRULE="(TIPO_PRODUTO=1) " STYLE="3" PROJECTION="0" ORDER="19" FIELDLABEL="Produtos" NESTEDNAME="MILLENIUM!SHOP.SOLICITACOES_COMPRA.PRODUTO" GROUPNAME="Produtos" CTAB="0"/>
        <FIELD NAME="FORNECEDORES" FORMAT="R" FLAGS="1" VISIBILITYRULE="SOLICITACAO_COMPRA>0" STYLE="3" PROJECTION="0" ORDER="25" FIELDLABEL="Fornecedores" NESTEDNAME="MILLENIUM!SHOP.SOLICITACOES_COMPRA.FORNECEDOR" GROUPNAME="Cota&#231;&#245;es" CTAB="0"/>
        <FIELD NAME="ORCADO" FORMAT="B" SIZE="1" FLAGS="17" VISIBILITYRULE="EMERGENCIAL=FALSE" ENABLINGRULE="ALTERACAO=FALSE" DEFAULT="1" PROJECTION="0" ORDER="12" FIELDLABEL="Or&#231;ado" GROUPNAME="Controle Or&#231;ament&#225;rio" CTAB="0"/>
        <FIELD NAME="OBS" FLAGS="1" PROJECTION="0" ORDER="21" FIELDLABEL="Observa&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="VALOR_PRE_APROVADO" FORMAT="M" SIZE="17" FLAGS="1" DECIMALS="4" VISIBILITYRULE="TIPO_PRODUTO&lt;>1" ENABLINGRULE="ALTERACAO=FALSE" PROJECTION="0" ORDER="13" FIELDLABEL="Valor Estimado" GROUPNAME="Controle Or&#231;ament&#225;rio" CTAB="0"/>
        <FIELD NAME="INDICA_FORNECEDOR" FORMAT="B" SIZE="1" FLAGS="17" ENABLINGRULE="ALTERACAO=FALSE" DEFAULT="0" PROJECTION="0" ORDER="15" FIELDLABEL="Sim" GROUPNAME="Indica Fornecedor" CTAB="0"/>
        <FIELD NAME="NOME_FORNECEDOR_INDICADO" SIZE="100" FLAGS="1" VISIBILITYRULE="INDICA_FORNECEDOR=TRUE" ENABLINGRULE="ALTERACAO=FALSE" PROJECTION="0" ORDER="16" FIELDLABEL="Fornecedor" GROUPNAME="Indica Fornecedor" CTAB="0"/>
        <FIELD NAME="TELEFONE_FORNECEDOR_INDICADO" SIZE="15" FLAGS="1" VISIBILITYRULE="INDICA_FORNECEDOR=TRUE" ENABLINGRULE="ALTERACAO=FALSE" PROJECTION="0" ORDER="17" FIELDLABEL="Telefone" GROUPNAME="Indica Fornecedor" CTAB="0"/>
        <FIELD NAME="OBS_COMPRAS" FLAGS="1" VISIBILITYRULE="SOLICITACAO_COMPRA>0" ENABLINGRULE="ALTERACAO=FALSE" PROJECTION="0" ORDER="28" FIELDLABEL="Observa&#231;&#227;o(Compras)" GROUPNAME="Finaliza&#231;&#227;o Compra" CTAB="0"/>
        <FIELD NAME="MESES_CONTRATO" FORMAT="N" SIZE="8" FLAGS="1" VISIBILITYRULE="TIPO_PRODUTO=2" ENABLINGRULE="ALTERACAO=FALSE" PROJECTION="0" ORDER="7" FIELDLABEL="Meses Contrato" CTAB="0"/>
        <FIELD NAME="DATA_ENTREGA_SOLICITANTE" FORMAT="D" SIZE="10" FLAGS="17" ENABLINGRULE="ALTERACAO=FALSE" PROJECTION="0" ORDER="3" FIELDLABEL="Data Entrega(Prevista)" CTAB="0"/>
        <FIELD NAME="MOTIVO_CANCELAMENTO" SIZE="100" FLAGS="145" VISIBILITYRULE="MOTIVO_CANCELAMENTO&lt;>''" PROJECTION="0" ORDER="22" FIELDLABEL="Motivo Cancelamento" CTAB="0"/>
        <FIELD NAME="APROVADO" FORMAT="B" SIZE="1" FLAGS="145" VISIBILITYRULE="SOLICITACAO_COMPRA>0" DEFAULT="0" PROJECTION="0" ORDER="8" FIELDLABEL="Aprovado" CTAB="0"/>
        <FIELD NAME="CANCELADO" FORMAT="B" SIZE="1" FLAGS="145" VISIBILITYRULE="SOLICITACAO_COMPRA>0" DEFAULT="0" PROJECTION="0" ORDER="10" FIELDLABEL="Cancelado" CTAB="0"/>
        <FIELD NAME="ENTREGUE" FORMAT="B" SIZE="1" FLAGS="145" VISIBILITYRULE="SOLICITACAO_COMPRA>0" DEFAULT="0" PROJECTION="0" ORDER="9" FIELDLABEL="Entregue" CTAB="0"/>
        <FIELD NAME="COMPRADO" FORMAT="B" SIZE="1" FLAGS="1" VISIBILITYRULE="STATUS_WORKFLOW=103" DEFAULT="0" PROJECTION="0" ORDER="27" FIELDLABEL="Comprado" GROUPNAME="Finaliza&#231;&#227;o Compra" CTAB="0"/>
        <FIELD NAME="HISTORICO" FORMAT="R" FLAGS="129" STYLE="3" PROJECTION="0" ORDER="30" FIELDLABEL="Historico" NESTEDNAME="MILLENIUM!SHOP.SOLICITACOES_COMPRA.HISTORICO" GROUPNAME="Fluxo" CTAB="0"/>
        <FIELD NAME="ALTERACAO" FORMAT="B" SIZE="1" DEFAULT="0" PROJECTION="0" FIELDLABEL="Alteracao" CTAB="0"/>
        <FIELD NAME="STATUS_WORKFLOW" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Status Workflow" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT SOLICITACAO_COMPRA,
       COD_SOLICITACAO,
       DATA,
       FUNCIONARIO,
       EMERGENCIAL,
       TIPO_PRODUTO,
       ORCADO,
       OBS,
       VALOR_PRE_APROVADO,
       INDICA_FORNECEDOR,
       NOME_FORNECEDOR_INDICADO,
       TELEFONE_FORNECEDOR_INDICADO,
       OBS_COMPRAS,
       APROVADO,
       CANCELADO,
       ENTREGUE,
       DATA_ENTREGA_SOLICITANTE,
       MOTIVO_CANCELAMENTO,
       MESES_CONTRATO,
       COMPRADO,
       TRUE AS ALTERACAO,
       STATUS_WORKFLOW,
       #ROWSET({SELECT:PRODUTOS
                      P.PRODUTO,
                      P.QUANTIDADE,
                      P.UNITARIO
                FROM SF_SOLICITACAO_COMPRA_PRODUTOS P
                WHERE P.SOLICITACAO_COMPRA=:SOLICITACAO_COMPRA})
       #ROWSET({SELECT:FORNECEDORES
                      F.FORNECEDOR,
                      F.VALOR_COTACAO,
                      F.GANHOU
                FROM SF_SOLICITACO_COMPRA_FORNEC F
                WHERE F.SOLICITACAO_COMPRA=:SOLICITACAO_COMPRA})
                
       #ROWSET({#CALL:HISTORICO MILLENIUM!SHOP.SOLICITACOES_COMPRA.LISTAHISTORICOWF(SOLICITACAO_COMPRA=:SOLICITACAO_COMPRA)})
       
FROM SF_SOLICITACOES_COMPRA
WHERE SOLICITACAO_COMPRA=:SOLICITACAO_COMPRA;
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>