<?xml version="1.0"?>
<root>
  <OBJECT NAME="EMAIL">
    <METHOD NAME="Alterar_email" DESCRIPTION="Alterar E-mail" VERSION="2" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="ID" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Id" CTAB="0"/>
        <PARAM NAME="E_MAIL_TO" SIZE="100" FLAGS="1" PROJECTION="0" FIELDLABEL="E Mail To" CTAB="0"/>
      </PARAMS>
      <FIELDS/>
      <ACTIONSCRIPT>UPDATE MSG_USUARIOS MU SET MU.NOME=:E_MAIL_TO  WHERE MU.MSG_DADOS=:ID

</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>