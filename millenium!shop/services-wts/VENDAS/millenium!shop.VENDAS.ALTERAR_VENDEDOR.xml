<?xml version="1.0"?>
<root>
  <OBJECT NAME="VENDAS">
    <METHOD NAME="ALTERAR_VENDEDOR" DESCRIPTION="Alterar Vendedor de Pedido" VERSION="8" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="PEDIDOV" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Pedido" CTAB="0"/>
        <PARAM NAME="VENDEDOR" FORMAT="N" SIZE="8" FLAGS="17" PROJECTION="0" LOOKUP="VENDAS.VENDEDORES_LISTA" LOOKUPKEY="FUNCIONARIO" LOOKUPDISPLAY="NOME" LOOKUPCODE="COD_FUNCIONARIO" FIELDLABEL="Vendedor" CTAB="0"/>
      </PARAMS>
      <FIELDS/>
      <ACTIONSCRIPT>UPDATE PEDIDO_VENDA PV SET PV.VENDEDOR=:VENDEDOR WHERE PV.PEDIDOV=:PEDIDOV
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>