<?xml version="1.0"?>
<root>
  <OBJECT NAME="PREFATURAMENTOS">
    <METHOD NAME="INFORMAR_SEPARADOR" DESCRIPTION="Informar Separador" VERSION="18" INTFTYPE="5" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="PREFATURAMENTO" SIZE="255" FLAGS="17" STYLE="6" PROJECTION="0" ORDER="2" LOOKUP="PREFATURAMENTOS.LISTA_PREFAT_PODE_CONFERIR" LOOKUPKEY="PREFATURAMENTO" LOOKUPDISPLAY="NOME" LOOKUPCODE="NUMERO" FIELDLABEL="Prefaturamento" CTAB="0"/>
        <PARAM NAME="FUNCIONARIO" FORMAT="N" SIZE="8" FLAGS="17" PROJECTION="0" ORDER="1" LOOKUP="PREFATURAMENTOS.LISTA_SEPARADORES" LOOKUPKEY="FUNCIONARIO" LOOKUPDISPLAY="NOME" LOOKUPCODE="COD_FUNCIONARIO" FIELDLABEL="Funcionario" CTAB="0"/>
      </PARAMS>
      <FIELDS/>
      <ACTIONSCRIPT>UPDATE PREFATURAMENTOS PF
SET PF.FUNCIONARIO=:FUNCIONARIO, PF.DATA_SEPARACAO=#NOW(),PF.CONFERINDO='T'
WHERE PF.PREFATURAMENTO in #replace(:PREFATURAMENTO) and pf.conferindo='F'

</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>