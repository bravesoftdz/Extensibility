<?xml version="1.0"?>
<root>
  <OBJECT NAME="PRODUCAO">
    <METHOD NAME="ListaHistSituacaoAut" DESCRIPTION="Hist&#243;rico Situa&#231;&#245;es Pagamento" VERSION="14" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="MOVIMENTO_PRODUCAO" SIZE="500" PROJECTION="0" FIELDLABEL="Movimento Producao" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="DATA" FORMAT="H" SIZE="18" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Data" CTAB="0"/>
        <FIELD NAME="DESCRICAO" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="2" FIELDLABEL="Descri&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="USUARIO" SIZE="50" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Usu&#225;rio" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT DATA,DESCRICAO,USUARIO
FROM MNR_HIST_SITUACAO_AUT
WHERE MOVIMENTO_PRODUCAO IN (#REPLACE(:MOVIMENTO_PRODUCAO))
GROUP BY DATA,DESCRICAO,USUARIO
ORDER BY DATA
</ACTIONSCRIPT>
    </METHOD>
